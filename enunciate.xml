<enunciate label="heliumv"
	 xsi:noNamespaceSchemaLocation="http://enunciate.codehaus.org/schemas/enunciate-1.29.xsd">
<!--
	<deployment host="www.ifyouwannabecool.com" context="api"/>
 -->
 
    <api-classes>
    	<exclude pattern="com.heliumv.api.cc.*" />
    	<exclude pattern="com.heliumv.api.delivery.*" />
    	<exclude pattern="com.heliumv.api.internal.*" />
    </api-classes>
 <!--
	<namespaces>
		<namespace id="api" uri="http://api.ifyouwannabecool.com/"/>
		<namespace id="link" uri="http://api.ifyouwannabecool.com/link"/>
		<namespace id="persona" uri="http://api.ifyouwannabecool.com/persona"/>
	</namespaces>
 -->
 
	<services>
    	<rest defaultRestSubcontext="/restapi/services/rest" />
	</services>
	
	<modules>
		<docs title="HELIUM V REST API" copyright="heliumv.com">
		</docs>
		<!-- Disable all the client generation tools -->
    	<basic-app disabled="true" />
    	<c disabled="true" />
    	<csharp disabled="true" />
    	<java-client disabled="false" />
	    <jaxws-client disabled="false" />
	    <jaxws-ri disabled="false" />
	    <jaxws-support disabled="false" />
	    <jersey disabled="false" />
	    <obj-c disabled="true" />
	    <xml disabled="true" />
	    <ruby disabled="true" />
	    <php disabled="true" />
